}}},this)
}this.disconnected(d)
},disconnected:function(c){this.value.socialConfig=null;
b.Squarespace.DialogFieldGenerators.connect.prototype.disconnected.call(this,c)
},onChange:function(){var c={};
c[this.oauthOwner+"Id"]=this.ownerId;
b.Data.get({url:"/api/commondata/GetSocialConfig",data:c,success:function(e){var f=e.socialConfig;
var d=f.serviceProviderEnum;
if(f&&!f.oauthError&&d==this.value.serviceProviderId){this.value.socialConfig=f;
this.value.username=f.profileUserName;
this.connected(f.profileUserName);
this.panel.fire("datachange",this)
}else{this.value.socialConfig=null;
this.disconnected();
this.panel.fire("datachange",this);
if(f.oauthError){console.log(f.oauthErrorMessage);
new b.Squarespace.Alert({position:"anchor",anchorEl:this.html}).show("External Authentication Error",f.oauthErrorMessage)
}else{new b.Squarespace.Alert({position:"anchor",anchorEl:this.html}).show("External Authentication Error","Error authenticating with external service.")
}}}},this)
}});
