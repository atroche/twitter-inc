},connected:function(c){if(this.progressTimer!==undefined){this.progressTimer.cancel()
}this.isConnected=true;
c=c||this.value.username;
this.message.set("text",this.connectedMessage+c);
this.html.removeClass("inprogress").removeClass("notconnected").addClass("connected");
this.fire("change",this)
},disconnect:function(c){if(!this.isConnected){return
}this.disconnected()
},disconnected:function(){this.value.username=null;
this.isConnected=false;
this.message.set("text","Click to connect");
this.html.removeClass("inprogress").removeClass("connected").addClass("notconnected");
this.fire("change",this)
},getValue:function(){},getType:function(){return"connect"
}});
