b.Squarespace.DialogFieldGenerators.emailConnect=Class.extend(b.Squarespace.DialogFieldGenerators.connect,{initialize:function(f,d,e){f.service="email";
f.connectedMessage="Sending to ";
this._super(f,d,e);
this.html.addClass("field-halfwidth");
var h="email";
