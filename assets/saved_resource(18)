}
}else{if(this.xdir=="left"&&this.isLeftViolation()){this.xdir="right";
this.getX=function(){return this.x+this.w+this.getXO()
}
}}if(this.ydir=="bottom"&&this.isBottomViolation()){this.ydir="top";
