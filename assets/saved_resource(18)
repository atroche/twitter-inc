var c=this.initialData[this.config.name];
if(c&&c[h]){g[h]=c[h]
}this.disconnectedField=new b.Squarespace.DialogFieldGenerators.text({title:"Email Address",name:h},g,this.panel);
this.disconnectedContainer.append(this.disconnectedField.html);
this.disconnectedField.control.on("click",function(i){i.stopPropagation()
});
this.disconnectedField.control.on("blur",function(i){this.connect()
},this);
this.disconnectedField.on("keyup-enter",this.connect,this)
},getValue:function(){if(this.disconnectedField){this.value[this.disconnectedField.getName()]=this.disconnectedField.getValue()
}this.value.connected=this.isConnected;
return this.value
},validateEmail:function(c){return a.test(c)
},connect:function(){this.value.username=this.disconnectedField.getValue();
if(this.value.username&&this.value.username!==""){this.inProgress();
if(this.validateEmail(this.value.username)){this.connected();
this.onStorageChange()
}else{this.disconnected();
this.invalidEmailError()
}}},disconnect:function(){this.disconnectedField.setValue("");
this._super();
this.onStorageChange()
},onStorageChange:function(){this.panel.fire("datachange",this);
var c=this.panel.getAnchorData();
if(b.Lang.isFunction(c.fire)){c.fire("form-storage-change",this,this.panel)
}},onError:function(c){this.invalidEmailError()
},invalidEmailError:function(){this.disconnected();
new b.Squarespace.Alert().show("Invalid Email Address","You must provide a valid email address.")
}})
