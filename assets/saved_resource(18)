},getMCAccount:function(){if(!this.value||!this.value.socialConfig){return this.mcError()
}b.Data.get({url:"/api/external/GetMailchimpAccountDetail",data:{connectedAccountId:this.value.socialConfig.externalAuthenticationId},success:function(c){if(c&&c.username){this.value.username=c.username;
this.value.socialConfig.profileUserName=c.username;
this.isConnected=true
}else{this.mcError();
