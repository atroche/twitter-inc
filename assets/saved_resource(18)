}},this);
this.container.set("innerHTML","");
this.container.append(this.field.html);
this.control=this.field.control;
this.field.setValue(b.Object.keys(e)[0]);
var c=this.initialData[this.config.name];
if(c&&c[this.field.getName()]){this.field.setValue(c[this.field.getName()])
}}},onStorageChange:function(){if(this.initializing){return
}this.fire("dataChange");
this.panel.fire("datachange",this);
var c=this.panel.getAnchorData();
if(!b.Lang.isUndefined(c)&&b.Lang.isFunction(c.fire)){c.fire("form-storage-change",this)
}}});
