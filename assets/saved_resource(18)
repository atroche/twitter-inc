}this.getMCLists()
},failure:function(c){this.mcError()
}},this)
},getMCLists:function(){if(!this.value||!this.value.socialConfig){return this.mcError()
}b.Data.get({url:"/api/external/GetMailchimpLists",data:{connectedAccountId:this.value.socialConfig.externalAuthenticationId},success:this.displayMCLists,failure:function(){this.mcError()
}},this);
b.Squarespace.DialogFieldGenerators.connect.prototype.connected.call(this);
this.panel.fire("datachange",this)
},displayMCLists:function(e){if(e){var f="list";
